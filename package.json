{
    "name": "gh-pages-app",
    "version": "0.1.0",
    "private": true,
    "homepage": "https://john-di.github.io/",
    "scripts": {
        "view": "rm -rf node_modules/gh-pages/.cache && gh-pages -b master -d dist",
        "save": "rm -rf node_modules/gh-pages/.cache && gh-pages -b develop -d src",
        "predeploy": "webpack --env.environment=production --config webpack.config.js",
        "deploy": "npm run save && npm run view",
        "build": "webpack --env.environment=production --config webpack.config.js",
        "dev": "webpack --env.environment=development --config webpack.config.js",
        "devserver": "webpack-dev-server --env.environment=development --config webpack.config.js"
    },
    "devDependencies": {
        "@babel/cli": "^7.7.7",
        "@babel/core": "^7.7.7",
        "@babel/preset-env": "^7.7.7",
        "@babel/preset-react": "^7.7.4",
        "autoprefixer": "^9.7.3",
        "babel-loader": "^8.0.6",
        "clean-webpack-plugin": "^3.0.0",
        "copy-webpack-plugin": "^5.1.1",
        "css-loader": "^3.4.0",
        "eslint": "^6.8.0",
        "file-loader": "^5.0.2",
        "fsevents": "^2.1.2",
        "gh-pages": "^2.1.1",
        "html-loader": "^0.5.5",
        "html-webpack-plugin": "^3.2.0",
        "mini-css-extract-plugin": "^0.9.0",
        "node-sass": "^4.13.0",
        "postcss-loader": "^3.0.0",
        "sass": "^1.24.0",
        "sass-loader": "^8.0.0",
        "sass-mq": "^5.0.1",
        "style-loader": "^1.1.2",
        "tiny-slider-react": "^0.4.0",
        "url-loader": "^3.0.0",
        "webpack": "^4.41.4",
        "webpack-cli": "^3.3.10",
        "webpack-dev-server": "^3.10.1",
        "webpack-md5-hash": "0.0.6",
        "webpack-node-externals": "^1.7.2"
    },
    "dependencies": {
        "core-js": "^3.6.1",
        "react": "^16.12.0",
        "react-dom": "^16.12.0"
    }
}
